[include]
src/**/*.js
[ignore]
.*/node_modules
[libs]
declarations
[options]
module.name_mapper='js'->'<PROJECT_ROOT>/src/js/'
module.name_mapper='vendor'->'<PROJECT_ROOT>/build/vendor/'
module.name_mapper='utils'->'<PROJECT_ROOT>/src/js/utils/'
module.name_mapper='stores'->'<PROJECT_ROOT>/src/js/stores/'
module.name_mapper='actions'->'<PROJECT_ROOT>/src/js/actions/'
module.name_mapper='constants'->'<PROJECT_ROOT>/src/js/constants/'
module.name_mapper='components'->'<PROJECT_ROOT>/src/js/components/'
module.name_mapper='alt'->'<PROJECT_ROOT>/build/vendor/alt/dist/alt.min'
module.name_mapper='react'->'<PROJECT_ROOT>/build/vendor/react/react.min'
module.name_mapper='react-dom'->'<PROJECT_ROOT>/build/vendor/react/react-dom.min'
module.name_mapper='esri\/.*'->'<PROJECT_ROOT>/src/js/config.js'
module.name_mapper='dojo\/.*'->'<PROJECT_ROOT>/src/js/config.js'
module.name_mapper='dijit\/.*'->'<PROJECT_ROOT>/src/js/config.js'
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
